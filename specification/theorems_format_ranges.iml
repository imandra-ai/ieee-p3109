let is_within (f : Format.t) (x : Float8.t) : bool =
  let open Float8.NaNOrExReal in
  let _, _, _, fmax = Format.get_format_parameters f in
  match Float8.decode f x with
  | Ok NaN
  | Ok (XR NINF)
  | Ok (XR PINF) -> true
  | Ok (XR r) -> R Real.(- fmax) <=. r && r <=. R fmax
  | _ -> false

let rec forall_in_range (p : int -> bool) (l : int) (u : int) =
  if l > u then
    true
  else
    p l && forall_in_range p (l + 1) u
  [@@measure Ordinal.of_int (u - l)]

theorem forall_elim (p: int -> bool) (l : int) (u : int) (x : int) =
  forall_in_range p l u ==> (l <= x && x <= u ==> p x)
  [@@by auto]

theorem wfr_neg (f : Format.t) (x : Float8.t) =
  x < 0 ==> is_within f x [@trigger]
  [@@by auto]
  [@@fc]

theorem wfr_above (f : Format.t) (x : Float8.t) =
  let k, _, _, _ = Format.get_format_parameters f in
  (x > ipow2 k) ==> is_within f x [@trigger]
  [@@disable Format.get_format_parameters]
  [@@by auto]
  [@@fc]

